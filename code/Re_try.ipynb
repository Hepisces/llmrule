    "def calculate(df,index):\n",
    "    path=df[\"path\"][index]\n",
    "    name=df[\"name\"][index]\n",
    "    to_calculate=pd.read_csv(path)\n",
    "    to_calculate[\"relation\"]=to_calculate[\"relation\"].apply(lambda x: x.replace('.',''))\n",
    "    sum=0\n",
    "    for i in range(len(to_calculate)):\n",
    "        sum+=to_calculate[\"relation\"][i]==to_calculate[\"target\"][i]\n",
    "    sum/=len(to_calculate)\n",
    "    df.loc[index,\"result\"]=sum\n",
    "    print(\"-------------\")\n",
    "    print(f\"name:{name}:\\n result:{sum}\\n\\n\")\n",
    "    "
   ]
